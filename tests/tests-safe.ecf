<?xml version="1.0" encoding="ISO-8859-1"?>
<system xmlns="http://www.eiffel.com/developers/xml/configuration-1-11-0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.eiffel.com/developers/xml/configuration-1-11-0 http://www.eiffel.com/developers/xml/configuration-1-11-0.xsd" name="nino_tests" uuid="0A0CF285-7536-41B4-9E4D-8EDFAA9A4166">
	<target name="nino_tests">
		<root class="ANY" feature="default_create"/>
		<file_rule>
			<exclude>/.git$</exclude>
			<exclude>/EIFGENs$</exclude>
			<exclude>/.svn$</exclude>
		</file_rule>
		<option warning="true" full_class_checking="true" is_attached_by_default="true" void_safety="transitional" syntax="provisional">
		</option>
		<setting name="concurrency" value="none"/>
		<library name="base" location="$ISE_LIBRARY\library\base\base-safe.ecf"/>
		<library name="http_client" location=".\ewf\library\client\http_client\http_client-safe.ecf" readonly="false"/>
		<library name="nino" location="..\nino-safe.ecf" readonly="false"/>
		<library name="testing" location="$ISE_LIBRARY\library\testing\testing-safe.ecf"/>
		<library name="thread" location="$ISE_LIBRARY\library\thread\thread-safe.ecf"/>
		<cluster name="echo" location=".\echo\" recursive="true"/>
		<tests name="src" location=".\src\" recursive="true"/>
	</target>
	<target name="nino_tests_thread" extends="nino_tests">
		<setting name="concurrency" value="thread"/>
	</target>
	<target name="nino_tests_scoop" extends="nino_tests">
		<setting name="concurrency" value="scoop"/>
	</target>
	<target name="echo_common" abstract="true">
		<file_rule>
			<exclude>/.git$</exclude>
			<exclude>/EIFGENs$</exclude>
			<exclude>/.svn$</exclude>
		</file_rule>
		<option debug="true" warning="true" full_class_checking="true" is_attached_by_default="true" void_safety="transitional" syntax="provisional">
			<debug name="nino" enabled="true"/>
			<debug name="pool" enabled="true"/>
			<assertions precondition="true" postcondition="true" check="true"/>
		</option>
		<setting name="executable_name" value="echo"/>
		<library name="base" location="$ISE_LIBRARY\library\base\base-safe.ecf"/>
		<library name="nino" location="..\nino-safe.ecf" readonly="false"/>
		<library name="testing" location="$ISE_LIBRARY\library\testing\testing-safe.ecf"/>
		<cluster name="echo" location=".\echo\" recursive="true"/>
	</target>
	<target name="echo" extends="echo_common">
		<root class="ECHO" feature="make_and_launch"/>
		<setting name="concurrency" value="none"/>
	</target>
	<target name="echo_thread" extends="echo_common">
		<root class="ECHO" feature="make_and_launch"/>
		<setting name="concurrency" value="thread"/>
	</target>
	<target name="echo_scoop" extends="echo_common">
		<root class="ECHO" feature="make_and_launch"/>
		<setting name="concurrency" value="scoop"/>
	</target>
</system>
